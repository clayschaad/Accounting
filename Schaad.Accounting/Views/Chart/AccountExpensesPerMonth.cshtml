<div class="row">
    <div class="col-lg-12">
        <div id="lineChartExpenses"></div>
    </div>
</div>   
<hr />  

@section scripts
{
    <script>
        $(function () {

            helper.get('/Chart/GetAccountExpensesOverTimeChart/@ViewData["AccountId"]', function (data) {
                helper.showColumnChart('#lineChartExpenses', '@ViewData["ChartTitle"]', 'Vergleich aktuelles/letztes Jahr', 'CHF', data);
            });
        });
    </script>
}
